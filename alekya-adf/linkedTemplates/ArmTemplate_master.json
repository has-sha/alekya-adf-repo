{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "alekya-adf"
		},
		"aimaster_sftp_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'aimaster_sftp'"
		},
		"AimasterSftpPassword_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://AlekyaKeyvault.vault.azure.net/"
		},
		"aimaster_sftp_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "aimaster.live"
		},
		"aimaster_sftp_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dh_y2u944"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "alekya-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"aimaster_sftp_password": {
						"value": "[parameters('aimaster_sftp_password')]"
					},
					"AimasterSftpPassword_properties_typeProperties_baseUrl": {
						"value": "[parameters('AimasterSftpPassword_properties_typeProperties_baseUrl')]"
					},
					"aimaster_sftp_properties_typeProperties_host": {
						"value": "[parameters('aimaster_sftp_properties_typeProperties_host')]"
					},
					"aimaster_sftp_properties_typeProperties_userName": {
						"value": "[parameters('aimaster_sftp_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}